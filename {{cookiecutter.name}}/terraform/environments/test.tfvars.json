{% if "test" in cookiecutter._aip_project_envs -%}
{
    "project": "{{cookiecutter._aip_project_envs.test.project_id}}",
    "region": "{{cookiecutter.zone.rsplit('-', 1)[0]}}",
    "zone": "{{cookiecutter.zone}}",
    "bucket": {
        "name": "{{cookiecutter._aip_project_envs.test.project_id}}",
        "location": "{{cookiecutter.zone|gcs_multi_location}}"
    },
    "github_repo": {
        "base_url": "{{cookiecutter._aip_repo_info.url.split('://')[0]}}://{{cookiecutter._aip_repo_info.url.split('://')[1].split('/')[0]}}",
        "owner": "{{cookiecutter._aip_repo_info.name.split('/')[0]}}",
        "name": "{{cookiecutter._aip_repo_info.name.split('/')[1]}}",
        "token": null
    },
    "ci_cd_triggers": {
        "master": {
            "config": "cloudbuild.yaml",
            "substitutions": {
                "_SOMEVARIABLE": "{{cookiecutter.name}}"
            },
            "branch": "master",
            "pull_request": false
        },
        "master-pr": {
            "config": "cloudbuild_pr.yaml",
            "substitutions": {
                "_SOMEVARIABLE": "{{cookiecutter.name}}"
            },
            "branch": "master",
            "pull_request": true
        }
    },
    "initial_builds": {
        "master": {
            "config": "cloudbuild.yaml",
            "substitutions": {
                "_SOMEVARIABLE": "{{cookiecutter.name}}"
            },
            "branch": "master"
        }
    }
}
{%- endif %}
